<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:mr3="http://mr3.sourceforge.net#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://mr3.sourceforge.net#">
  <rdfs:Class rdf:about="http://www.w3.org/2000/01/rdf-schema#Resource"/>
  <rdfs:Class rdf:ID="MarriedWoman">
    <rdfs:subClassOf>
      <rdfs:Class rdf:ID="Woman"/>
    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  <rdfs:Class rdf:about="#Woman">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
  </rdfs:Class>
  <owl:DatatypeProperty rdf:ID="hasMaidenName">
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:resource="#MarriedWoman"/>
  </owl:DatatypeProperty>
  <rdf:Description rdf:ID="Karen">
    <mr3:hasMaidenName rdf:datatype="http://www.w3.org/2001/XMLSchema#Name"
    >Stephens</mr3:hasMaidenName>
  </rdf:Description>
</rdf:RDF>
